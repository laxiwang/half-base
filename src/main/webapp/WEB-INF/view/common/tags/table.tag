@/*
    表格标签的参数说明:

    id : table表格的id
@*/
 <div style="overflow:scroll;" style="z-index: 1;">
<table id="${id}" data-mobile-responsive="true" data-click-to-select="true"  style="${style}">
    <thead>
        <tr>
            <th data-field="selectItem" data-checkbox="true"></th>
        </tr>
    </thead>
</table>
</div>